<!DOCTYPE html>
<html>

<head>
    <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
    <link rel="stylesheet" type="text/css" href="/stylesheets/form.css">
</head>

<body>
    <%- include('partials/navbar') %>
        <div class="container" id="aboutContainer">
            <% for (member of membersInfo) {%>
                <a href="/about/<%= member.lname%>">
                    <div class="row">
                        <div class="col">
                            <div class= "bg-dark text-white col" id="aboutCard">
                                <img src="<%= member.img %>" class="m-3 card-img-top" id="aboutImg">
                                <div class="card-body">
                                    <h5 class="card-title font-monospace fs-3">
                                        <%= member.name %>
                                    </h5>
                                    <p class="card-text">
                                        <%= member.role%>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </a>
                <% } %>
        </div>
        <%- include('partials/footer') %>


            <script src="/javascripts/bootstrap.bundle.js"></script>
</body>

</html>