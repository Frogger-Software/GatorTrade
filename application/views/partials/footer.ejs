<% if(locals.logged) { %>
    <script>
        let ele = document.getElementById('logout-button');
        if(ele) {
          ele.onclick = (even) => {
            fetch('/users/logout', {
              method:"POST"
            })
            .then((data) => {
              location.replace('/');
            })
          }
        }
      </script>
<% } %>

<div class="mt-2">
